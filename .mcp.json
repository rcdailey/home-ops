{
  "mcpServers": {
    "hass": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-proxy",
        "https://ha.dailey.app/mcp_server/sse"
      ],
      "env": {
        "API_ACCESS_TOKEN": "${HASS_TOKEN}"
      }
    },
    "k8s": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "kubernetes-mcp-server@latest"
      ],
      "env": {}
    }
  }
}
