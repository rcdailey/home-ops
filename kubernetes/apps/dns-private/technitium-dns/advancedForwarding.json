{
  "appPreference": 200,
  "enableForwarding": true,
  "proxyServers": [
    {
      "name": "local-proxy",
      "type": "socks5",
      "proxyAddress": "localhost",
      "proxyPort": 1080,
      "proxyUsername": null,
      "proxyPassword": null
    }
  ],
  "forwarders": [
    {
      "name": "quad9-doh",
      "proxy": null,
      "dnssecValidation": true,
      "forwarderProtocol": "Https",
      "forwarderAddresses": ["https://dns.quad9.net/dns-query (9.9.9.9)"]
    },
    {
      "name": "nextdns-main",
      "proxy": null,
      "dnssecValidation": true,
      "forwarderProtocol": "Quic",
      "forwarderAddresses": ["${NEXTDNS_MAIN_ENDPOINT}"]
    },
    {
      "name": "nextdns-privacy",
      "proxy": null,
      "dnssecValidation": true,
      "forwarderProtocol": "Quic",
      "forwarderAddresses": ["${NEXTDNS_PRIVACY_ENDPOINT}"]
    },
    {
      "name": "nextdns-guest",
      "proxy": null,
      "dnssecValidation": true,
      "forwarderProtocol": "Quic",
      "forwarderAddresses": ["${NEXTDNS_GUEST_ENDPOINT}"]
    },
    {
      "name": "nextdns-kids",
      "proxy": null,
      "dnssecValidation": true,
      "forwarderProtocol": "Quic",
      "forwarderAddresses": ["${NEXTDNS_KIDS_ENDPOINT}"]
    }
  ],
  "networkGroupMap": {
    "192.168.1.0/24": "main-lan",
    "192.168.2.0/24": "privacy",
    "192.168.3.0/24": "kids",
    "192.168.4.0/24": "guest",
    "192.168.7.0/24": "privacy",
    "0.0.0.0/0": "default",
    "[::]/0": "default"
  },
  "groups": [
    {
      "name": "main-lan",
      "enableForwarding": true,
      "forwardings": [
        {
          "forwarders": ["nextdns-main"],
          "domains": ["*"]
        }
      ]
    },
    {
      "name": "privacy",
      "enableForwarding": true,
      "forwardings": [
        {
          "forwarders": ["nextdns-privacy"],
          "domains": ["*"]
        }
      ]
    },
    {
      "name": "kids",
      "enableForwarding": true,
      "forwardings": [
        {
          "forwarders": ["nextdns-kids"],
          "domains": ["*"]
        }
      ]
    },
    {
      "name": "guest",
      "enableForwarding": true,
      "forwardings": [
        {
          "forwarders": ["nextdns-guest"],
          "domains": ["*"]
        }
      ]
    },
    {
      "name": "default",
      "enableForwarding": true,
      "forwardings": [
        {
          "forwarders": ["quad9-doh"],
          "domains": ["*"]
        }
      ]
    }
  ]
}
